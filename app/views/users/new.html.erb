<h2>Novo atendente</h2>
<% if @user.errors.any? %>
  <ul>
    <% @user.errors.full_messages.each do |full_message| %>
      <li><%= full_message %></li>
    <% end %>
  </ul>
<% end %>
<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label 'email' %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label 'senha' %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Criar' %>
  </p>
<% end %>
<hr/>
<%= link_to 'Sair', destroy_admin_session_path, method: :delete %>
